<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="503dp"
        android:layout_height="1069dp"
        android:layout_marginStart="-50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_479"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="447dp"
        android:layout_height="252dp"
        android:layout_marginTop="68dp"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_358"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Button2"
        android:layout_width="352dp"
        android:layout_height="70dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="312dp"
        android:backgroundTint="#1A529D"
        android:fontFamily="@font/montserrat"
        android:onClick="loginp"
        android:text="Login"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="352dp"
        android:layout_height="70dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="388dp"
        android:backgroundTint="@color/white"
        android:fontFamily="@font/montserrat"
        android:onClick="registerp"
        android:text="Register"
        android:textColor="#4E5355"
        android:textSize="18sp"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#1A529D"
        app:strokeWidth="2dp" />

    <TextView
        android:id="@+id/textView200"
        android:layout_width="151dp"
        android:layout_height="31dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="480dp"
        android:fontFamily="@font/montserrat"
        android:text="Kebaikan Alam"
        android:textAlignment="center"
        android:textColor="#4D4D4D"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView201"
        android:layout_width="160dp"
        android:layout_height="31dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="508dp"
        android:fontFamily="@font/montserrat"
        android:text="Kebaikan Hidup"
        android:textAlignment="center"
        android:textColor="#4D4D4D"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="448dp"
        android:layout_height="356dp"
        android:layout_marginTop="572dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_360"
        tools:layout_editor_absoluteX="1dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="385dp"
        android:layout_height="402dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="524dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/aqua_png_13_photoroom_1" />

</androidx.constraintlayout.widget.ConstraintLayout>